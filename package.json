{
  "name": "trabalho-redes",
  "version": "1.0.0",
  "description": "",
  "main": "build/server/main.js",
  "scripts": {
    "start": "ts-node bin/start.ts",
    "electron:start": "ts-node bin/electron.ts",
    "react": "ts-node bin/react.ts",
    "build": "ts-node bin/build.ts",
    "electron": "npm run start && electron .",
    "electron:build": "electron-builder --win --publish never -c.extraMetadata.main=build/server/main.js --config=electron-builder.yml",
    "electron:build:32bits": "electron-builder --win --publish never -c.extraMetadata.main=build/server/main.js --config=electron-builder.yml --ia32",
    "electron:deploy": "electron-builder --win --publish always -c.extraMetadata.main=build/server/main.js --config=electron-builder.yml",
    "release": "npm run build && npm run electron:build",
    "deploy": "npm run build && npm run electron:deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.1",
    "electron": "^19.0.4",
    "electron-builder": "^23.1.0",
    "html-webpack-plugin": "^5.5.0",
    "path-browserify": "^1.0.1",
    "sass-loader": "^12.6.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "electron-log": "^4.4.7",
    "electron-updater": "^5.0.5",
    "fs-extra": "^10.1.0",
    "react-scripts": "^5.0.1",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2"
  },
  "engines": {
    "node": "14.15.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
